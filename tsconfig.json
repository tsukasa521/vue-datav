{
    "compilerOptions": {
        "target": "esnext",
        "useDefineForClassFields": true,
        "module": "umd",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "jsxFactory": "h",
        "isolatedModules": false,
        "esModuleInterop": true,
        "lib": ["esnext", "dom"],
        "skipLibCheck": true
    }
}
